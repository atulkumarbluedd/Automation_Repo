<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" data-provider-thread-count="3">
	<!-- here we want to restrict only 3 threads to be executed in parallel by providing data-provider-thread-count-->
	<listeners>
		<listener class-name="SeleniumHandsOn.Listeners" />
	</listeners>
	<test thread-count="5" name="Test">
		<classes>
			<class name="SeleniumHandsOn.launchChrome" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
